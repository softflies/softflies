{
    "name": "softflies/softflies",
    "type": "project",
    "description": "The softflies Framework.",
    "keywords": ["framework", "softflies"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "softflies/framework": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}